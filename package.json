  "jupyterlab": {
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "jupyter_rtc_test"
        }
      }
    },
    "extension": "./lib/jupyterlab/index.js",
    "outputDir": "jupyter_rtc_test/labextension",
    "schemaDir": "schema"
  },
  "jupyter-releaser": {
    "hooks": {
      "before-build-npm": [
        "python -m pip install jupyterlab==4.0.0",
        "jlpm"
      ],
      "before-build-python": [
        "jlpm clean:all"
      ]
    }
