{
  "name": "@datalayer/jupyter-rtc-test",
  "version": "0.0.3",
  "description": "Test for Jupyter RTC.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "yjs",
    "rtc"
  ],
  "homepage": "https://github.com/datalayer/jupyter-rtc-test",
  "bugs": {
    "url": "https://github.com/datalayer/jupyter-rtc-test/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Datalayer",
    "email": "eric@datalayer.io"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/datalayer/jupyter-rtc-test.git"
  },
  "scripts": {
    "build": "jlpm build:lib && jlpm build:labextension:dev",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "build:prod": "jlpm clean && jlpm build:lib && jlpm build:labextension",
    "clean": "jlpm clean:lib",
    "clean:all": "jlpm clean:lib && jlpm clean:labextension && jlpm clean:lintcache",
    "clean:labextension": "rimraf datalayer/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:lintcache": "rimraf .eslintcache .stylelintcache",
    "eslint": "jlpm eslint:check --fix",
    "eslint:check": "eslint . --cache --ext .ts,.tsx",
    "hello": "ts-node src/hello.ts",
    "install:extension": "jlpm build",
    "jupyterlab": "run-p -c jupyterlab:*",
    "jupyterlab:start": "jupyter lab --watch --config=./dev/config/jupyter_server_config.py",
    "jupyterlab:watch": "run-p -c watch:src watch:labextension",
    "kill": "make kill || true",
    "lint": "jlpm stylelint && jlpm prettier && jlpm eslint",
    "lint:check": "jlpm stylelint:check && jlpm prettier:check && jlpm eslint:check",
    "prettier": "jlpm prettier:base --write --list-different",
    "prettier:base": "prettier \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "prettier:check": "jlpm prettier:base --check",
    "start": "run-p -c start:* -c start:ws:ypy",
    "start:jupyter-server": "cd ./dev/sh && ./start-jupyter-server.sh",
    "start:webpack": "webpack serve",
    "start:webrtc:yjs": "fkill -s :4444 && node src/webrtc/server/server-1.js",
    "start:ws:simple": "fkill -s :8899 && node --loader ts-node/esm src/ws/server/WSServerSimple.mts",
    "start:ws:yjs": "fkill -s :1234 && YPERSISTENCE=./data-server ts-node-esm src/ws/server/WSServerYjs1.mts",
    "start:ws:ypy": "fkill -s :1234 && python jupyter_rtc_test/tests/start_y_websocket.py",
    "stylelint": "jlpm stylelint:check --fix",
    "stylelint:check": "stylelint --cache \"style/**/*.css\"",
    "test": "run-s test:*",
    "test-coverage": "jest --coverage",
    "test:yjs": "run-p --race start:ws:simple start:ws:yjs start:webrtc:yjs wait:test:yjs",
    "test:ypy": "pip uninstall pytest-tornasync && pytest jupyter_rtc_test/tests --ignore=jupyter_rtc_test/tests/handler --asyncio-mode=auto -vv",
    "test:ypy:loop": "pip uninstall pytest-tornasync && while true; do pytest jupyter_rtc_test/tests --ignore=jupyter_rtc_test/tests/handler --asyncio-mode=auto -vv; done",
    "test:ypy:handler": "pytest jupyter_rtc_test/tests/handler --asyncio-mode=strict -vv",
    "wait:test:yjs": "run-s wait:ws:simple wait:ws:yjs wait:webrtc:yjs && npx jest",
    "wait:test:ypy": "run-s wait:ws:ypy && pytest",
    "wait:webrtc:yjs": "wait-on tcp:4444",
    "wait:ws:simple": "wait-on tcp:8899",
    "wait:ws:yjs": "wait-on tcp:1234",
    "wait:ws:ypy": "wait-on tcp:1234",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w"
  },
  "dependencies": {
    "@datalayer/icons": "0.0.10",
    "@jupyterlab/application": "4.0.0-beta.0",
    "@jupyterlab/coreutils": "6.0.0-beta.0",
    "@jupyterlab/services": "7.0.0-beta.0",
    "@jupyterlab/settingregistry": "4.0.0-beta.0",
    "@primer/octicons-react": "18.2.0",
    "@primer/react": "35.22.0",
    "lodash.debounce": "4.0.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "4.4.1",
    "yjs": "13.5.47"
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-transform-modules-commonjs": "7.21.2",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.0",
    "@jupyterlab/builder": "4.0.0-beta.0",
    "@jupyterlab/launcher": "4.0.0-beta.0",
    "@jupyterlab/testutils": "4.0.0-beta.0",
    "@types/jest": "29.4.0",
    "@types/node": "18.11.9",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "4.8.1",
    "@typescript-eslint/parser": "4.8.1",
    "assert": "2.0.0",
    "babel-loader": "9.1.2",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "fkill-cli": "6.0.1",
    "html-webpack-plugin": "5.3.1",
    "jest": "29.4.3",
    "jest-playwright-preset": "3.0.1",
    "mkdirp": "1.0.3",
    "node-static": "0.7.11",
    "npm-run-all": "4.1.5",
    "playwright": "1.31.1",
    "prettier": "2.1.1",
    "rectangle-overlap": "2.0.0",
    "rimraf": "3.0.2",
    "style-loader": "2.0.0",
    "stylelint": "14.3.0",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-recommended": "6.0.0",
    "stylelint-config-standard": "~24.0.0",
    "stylelint-prettier": "2.0.0",
    "textarea-caret": "3.1.0",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "5.0.3",
    "wait-on": "7.0.1",
    "webpack": "5.74.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.3",
    "ws": "8.12.1",
    "y-codemirror": "3.0.1",
    "y-indexeddb": "9.0.9",
    "y-leveldb": "0.1.2",
    "y-protocols": "1.0.5",
    "y-webrtc": "10.2.4",
    "y-websocket": "1.4.5"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "jupyter_rtc_test"
        }
      }
    },
    "extension": "./lib/jupyterlab/index.js",
    "outputDir": "jupyter_rtc_test/labextension",
    "schemaDir": "schema"
  },
  "jupyter-releaser": {
    "hooks": {
      "before-build-npm": [
        "python -m pip install jupyterlab==4.0.0b0",
        "jlpm"
      ],
      "before-build-python": [
        "jlpm clean:all"
      ]
    }
  }
}
