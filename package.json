  "scripts": {
    "build": "jlpm build:lib && jlpm build:labextension:dev",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "build:prod": "jlpm clean && jlpm build:lib && jlpm build:labextension",
    "build:webpack": "webpack-cli build",
    "clean": "jlpm clean:lib",
    "clean:all": "jlpm clean:lib && jlpm clean:labextension && jlpm clean:lintcache",
    "clean:labextension": "rimraf datalayer/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:lintcache": "rimraf .eslintcache .stylelintcache",
    "eslint": "jlpm eslint:check --fix",
    "eslint:check": "eslint . --cache --ext .ts,.tsx",
    "hello": "ts-node src/hello.ts",
    "install:extension": "jlpm build",
    "jupyterlab": "run-p -c 'jupyterlab:*'",
    "jupyter-rtc-test": "jupyter rtc-test",
    "jupyterlab:start": "jupyter lab --watch --config=./dev/config/jupyter_server_config.py",
    "jupyterlab:watch": "run-p -c watch:src watch:labextension",
    "kill": "make kill || true",
    "lint": "jlpm stylelint && jlpm prettier && jlpm eslint",
    "lint:check": "jlpm stylelint:check && jlpm prettier:check && jlpm eslint:check",
    "prettier": "jlpm prettier:base --write --list-different",
    "prettier:base": "prettier \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "prettier:check": "jlpm prettier:base --check",
    "start": "run-p -c 'start:*' -c start:ws:ypy",
    "start:jupyter-server": "cd ./dev/sh && ./start-jupyter-server.sh",
    "start:webpack": "webpack serve",
    "start:webrtc:yjs": "fkill -s :4444 && node src/utils/webrtc-server/server-1.js",
    "start:ws:transparent": "fkill -s :8899 && node --loader ts-node/esm src/utils/ws-server/WSServerTransparent.mts",
    "start:ws:yjs": "fkill -s :1234 && YPERSISTENCE=./data-server ts-node-esm src/utils/ws-server/WSServerYjs.mts",
    "start:ws:ypy": "fkill -s :1234 && python jupyter_rtc_test/tests/start_y_websocket.py",
    "stylelint": "jlpm stylelint:check --fix",
    "stylelint:check": "stylelint --cache \"style/**/*.css\"",
    "test": "run-s 'test:*'",
    "test-coverage": "jest --coverage",
    "test:py": "pytest jupyter_rtc_test/tests --ignore=jupyter_rtc_test/tests/0_handler --asyncio-mode=auto -vv",
    "test:py:handler": "pytest jupyter_rtc_test/tests/0_handler --asyncio-mode=strict -vv",
    "test:py:loop": "bash -c 'while true; do pytest jupyter_rtc_test/tests --ignore=jupyter_rtc_test/tests/0_handler --asyncio-mode=auto -vv; done'",
    "test:js": "run-p --race start:ws:transparent start:ws:yjs start:webrtc:yjs run:test:jest",
    "run:test:jest": "run-s wait:ws:transparent wait:ws:yjs wait:webrtc:yjs && npx jest",
    "wait:webrtc:yjs": "wait-on tcp:4444",
    "wait:ws:transparent": "wait-on tcp:8899",
    "wait:ws:yjs": "wait-on tcp:1234",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w"
  },
